{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "functions": {
    "app.js": {
      "includeFiles": ["models/**"]
    }
  }
},
  "ignoreCommand": "echo 'Ignoring nothing'",  
  "functions": {
    "app.js": {
      "includeFiles": ["models/**"]
    }
  }
}
